--links=no
--tag-relative=yes
--sort=foldcase
--languages=-javascript,sql,css
--fields=+l
--python-kinds=-i
--exclude=.git
--exclude=.svn
--exclude=.tox
--exclude=logs
--exclude=Session.vim
--langdef=Go
--langmap=Go:.go
--regex-Go=/func([ \t]+\([^)]+\))?[ \t]+([a-zA-Z0-9_]+)/\2/d,func/
--regex-Go=/var[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,var/
--regex-Go=/type[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,type/
