[user]
	name = Rinat Shigapov
	email = rinatshigapov@gmail.com
	signingkey = 0xC600E8FB
[core]
	excludesfile = ~/.gitignore
[format]
	pretty = "%Cred%h%Creset %ad [%an <%ae>]	%Cgreen%s%d%Creset"
[log]
	date = short
[alias]
	a = add
	b = branch
	c = checkout
	cm = commit
	cma = commit -a
	p = pull
	d = diff
	l = log --graph
	s = status
	r = rebase
	rc = rebase --continue
	ctags = !test -x .git/hooks/ctags && .git/hooks/ctags
[svn]
	pushmergeinfo = 1
	rmdir = 1
[color]
	branch = true
	status = true
	interactive = true
	diff = true
	grep = true
	ui = true
[web]
	browser = chromium-browser
[http]
	postBuffer = 64M
[credential]
	helper = cache --timeout=30000
	branch = true
[push]
	default = current
[init]
	templatedir = ~/.git_template
[include]
	path = .gitconfig.local
